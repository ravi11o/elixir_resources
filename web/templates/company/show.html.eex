<h2>Show company</h2>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @company.title %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @company.description %>
  </li>

  <li>
    <strong>Cover/logo:</strong>
    <%= @company.cover %>
  </li>

  <li>
    <strong>Website:</strong>
    <%= @company.website %>
  </li>

</ul>

<%= link "Edit", to: company_path(@conn, :edit, @company) %>
<%= link "Back", to: company_path(@conn, :index) %>
